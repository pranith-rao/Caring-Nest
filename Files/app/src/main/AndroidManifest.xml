<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caringnest">

    <uses-permission android:name="android.permission.INTERNEETER" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CaringNest">
        <activity android:name=".About"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminLogs" />
        <activity android:name=".Confirmed" />
        <activity android:name=".ConfirmBooking" />
        <activity android:name=".UserAddDetails" />
        <activity android:name=".AdminMain" />
        <activity android:name=".AdminLogin" />
        <activity android:name=".AdminAddDetails" />
        <activity android:name=".UserMain" />
        <activity android:name=".MainSelect"></activity>
    </application>

</manifest>